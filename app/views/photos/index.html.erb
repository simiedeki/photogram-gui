<ul>
  <li>
    <a href="/users">Users</a>
  </li>
  <li>
    <a href="/photos">Photos</a>
  </li>
</ul>
<h1>List of Photos </h1>

<table border="1">
  <tr>
    <th>Image</th>
    <th>Owner</th>
    <th>Caption</th>
    <th>Posted</th>
    <th>Details</th>
  </tr>
  <% @list_of_photos.each do |a_photo| %>
  <tr>
    <td><img src="<%= a_photo.image %>"></td>
    <td><%= a_photo.poster.username %></td>
    <td><%= a_photo.caption %></td>
    <td><%= a_photo.created_at %></td>
    <td>
      <a href="/photos/<%= a_photo.id %>">Show details</a>
    </td>
  </tr>
  <% end %> <!-- Ensure the loop is closed correctly -->
</table>
